---
import Layout from "../layouts/Layout.astro";
import LandingHeader from "../components/LandingHeader.astro";
import TestHeader from "../components/TestHeader.astro";
import HeroSection from "../components/HeroSection.astro";
import ModelS from "../components/ModelS.astro";
import ModelThree from "../components/ModelThree.astro";
import ModelX from "../components/ModelX.astro";
import EnjoyTesla from "../components/EnjoyTesla.astro";
import PowerWall from "../components/PowerWall.astro";
import Accesories from "../components/Accesories.astro";
---

<Layout title="Tesla" description="pagina de tesla">
    <TestHeader />
    <LandingHeader />
    <main
        class="relative snap-mandatory snap-y h-screen w-screen overflow-y-auto overflow-x-hidden scroll-smooth min-w-[800px]"
    >
        <div class="snap-center">
            <HeroSection />
        </div>
        <div class="snap-center">
            <ModelThree />
        </div>
        <div class="snap-center">
            <ModelS />
        </div>
        <div class="snap-center">
            <ModelX />
        </div>
        <div class="snap-center">
            <EnjoyTesla />
        </div>
        <div class="snap-center">
            <PowerWall />
        </div>
        <div class="snap-center">
            <Accesories />
        </div>
    </main>
</Layout>
